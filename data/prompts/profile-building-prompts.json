{
  "profileBuildingPrompts": [
    {
      "id": "profile_name",
      "question": "Let's start building your profile! What's your first name? (This is what others will see)",
      "type": "text",
      "category": "profile_onboarding",
      "theme": "introduction",
      "tone": "friendly",
      "intendedUseCase": "profile_completion",
      "emotionalDepth": "low",
      "extractionRules": {
        "field": "name",
        "validation": "minimum 2 characters, letters only",
        "example": "Sarah"
      },
      "metadata": {
        "priority": 1,
        "isRequired": true,
        "fieldType": "critical"
      }
    },
    {
