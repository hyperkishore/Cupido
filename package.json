{
  "name": "cupido",
  "version": "1.2.40",
  "main": "index.ts",
  "scripts": {
    "docs:manifest": "node scripts/generate-docs-manifest.js",
    "prestart": "npm run docs:manifest",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo export --platform web",
    "prebuild": "npm run docs:manifest",
    "build:web": "expo export:web",
    "build:production": "NODE_ENV=production expo export --platform web",
    "serve": "npx serve dist",
    "deploy": "npm run build:production && npm run serve",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "npm run test:unit",
    "test:unit": "node tests/reflectionAiService.test.js && node tests/homeExperienceService.test.js && node tests/intelligentQuestionService.test.js",
    "web:test": "EXPO_NO_INTERACTIVE=1 expo start --web --port 8081",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
