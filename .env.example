# Cupido Environment Variables
# Copy this file to .env and fill in your values

# API Configuration
EXPO_PUBLIC_API_BASE_URL=https://api.cupido.app
EXPO_PUBLIC_ENVIRONMENT=development

# Claude AI Configuration
REACT_APP_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Third-party OAuth Apps (for social connections)
# LinkedIn
EXPO_PUBLIC_LINKEDIN_CLIENT_ID=
EXPO_PUBLIC_LINKEDIN_REDIRECT_URI=

# Instagram
EXPO_PUBLIC_INSTAGRAM_CLIENT_ID=
EXPO_PUBLIC_INSTAGRAM_REDIRECT_URI=

# Twitter
EXPO_PUBLIC_TWITTER_CLIENT_ID=
EXPO_PUBLIC_TWITTER_REDIRECT_URI=

# Spotify
EXPO_PUBLIC_SPOTIFY_CLIENT_ID=
EXPO_PUBLIC_SPOTIFY_REDIRECT_URI=

# YouTube
EXPO_PUBLIC_YOUTUBE_CLIENT_ID=
EXPO_PUBLIC_YOUTUBE_REDIRECT_URI=

# GitHub
EXPO_PUBLIC_GITHUB_CLIENT_ID=
EXPO_PUBLIC_GITHUB_REDIRECT_URI=

# Push Notifications
EXPO_PUBLIC_ONESIGNAL_APP_ID=

# Analytics
EXPO_PUBLIC_MIXPANEL_TOKEN=
EXPO_PUBLIC_SENTRY_DSN=

# Feature Flags
EXPO_PUBLIC_ENABLE_VOICE_NOTES=true
EXPO_PUBLIC_ENABLE_VIDEO_CALLS=false
EXPO_PUBLIC_ENABLE_AI_MATCHING=true
EXPO_PUBLIC_ENABLE_VOICE_INPUT=true
EXPO_PUBLIC_ENABLE_FEEDBACK_SYSTEM=true
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=false
EXPO_PUBLIC_ENABLE_ANALYTICS=false

# Rate Limiting
EXPO_PUBLIC_MAX_DAILY_REFLECTIONS=5
EXPO_PUBLIC_MAX_DAILY_MATCHES=10
EXPO_PUBLIC_MAX_DAILY_MESSAGES=100

# App Configuration
EXPO_PUBLIC_APP_NAME=Cupido
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_WEB_URL=http://localhost:8081

# Security
EXPO_PUBLIC_APP_SECRET=your_app_secret_here
JWT_SECRET=your_jwt_secret_here

# Development Settings
EXPO_PUBLIC_SHOW_DEV_TOOLS=false
EXPO_PUBLIC_DEBUG_MODE=false

# Twilio Configuration (For SMS/OTP when ready)
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here